+ '[' -d /usr/share/initramfs-tools/hooks/cryptroot ']'
+ tmpdeplist=' cryptroot'
+ deplist=' cryptroot'
+ for rs_x in '${runlist}'
+ pop_list_item cryptroot cryptroot
+ item=cryptroot
+ shift
+ set -- cryptroot
+ unset tmppop
+ for pop in '${@}'
+ '[' cryptroot = cryptroot ']'
+ continue
+ deplist=
+ for rs_x in '${runlist}'
+ pop_list_item kernelextras
+ item=kernelextras
+ shift
+ set --
+ unset tmppop
+ deplist=
+ for rs_x in '${runlist}'
+ pop_list_item keymap
+ item=keymap
+ shift
+ set --
+ unset tmppop
+ deplist=
+ for rs_x in '${runlist}'
+ pop_list_item legacylvm
+ item=legacylvm
+ shift
+ set --
+ unset tmppop
+ deplist=
+ for rs_x in '${runlist}'
+ pop_list_item lvm2
+ item=lvm2
+ shift
+ set --
+ unset tmppop
+ deplist=
+ for rs_x in '${runlist}'
+ pop_list_item thermal
+ item=thermal
+ shift
+ set --
+ unset tmppop
+ deplist=
+ for rs_x in '${runlist}'
+ pop_list_item udev
+ item=udev
+ shift
+ set --
+ unset tmppop
+ deplist=
+ for rs_x in '${runlist}'
+ pop_list_item udevhelper
+ item=udevhelper
+ shift
+ set --
+ unset tmppop
+ deplist=
+ for rs_x in '${runlist}'
+ pop_list_item usplash
+ item=usplash
+ shift
+ set --
+ unset tmppop
+ deplist=
+ for rs_x in '${runlist}'
+ pop_list_item uswsusp
+ item=uswsusp
+ shift
+ set --
+ unset tmppop
+ deplist=
+ for rs_x in '${runlist}'
+ pop_list_item cryptopenct
+ item=cryptopenct
+ shift
+ set --
+ unset tmppop
+ deplist=
+ for rs_x in '${runlist}'
+ pop_list_item cryptopensc
+ item=cryptopensc
+ shift
+ set --
+ unset tmppop
+ deplist=
+ eval 'array_cryptpassdev=""'
++ array_cryptpassdev=
++ render array_cryptpassdev
++ eval 'echo -n ${array_cryptpassdev}'
+++ echo -n
+ count_unsatisfied
+ set --
+ return 0
+ cnt=0
+ '[' 0 -eq 0 ']'
+ runlist=' cryptroot kernelextras keymap legacylvm lvm2 thermal udev udevhelper usplash uswsusp cryptopenct cryptopensc cryptpassdev'
+ pop_list_item cryptpassdev cryptpassdev
+ item=cryptpassdev
+ shift
+ set -- cryptpassdev
+ unset tmppop
+ for pop in '${@}'
+ '[' cryptpassdev = cryptpassdev ']'
+ continue
+ initlist=
+ i=0
+ '[' 0 -eq 3 ']'
+ '[' 0 -ne 0 ']'
+ call_scripts
+ for cs_x in '${runlist}'
+ '[' n = y ']'
+ /usr/share/initramfs-tools/hooks/cryptroot
+ '[' -e /conf/param.conf ']'
+ for cs_x in '${runlist}'
+ '[' n = y ']'
+ /usr/share/initramfs-tools/hooks/kernelextras
+ '[' -e /conf/param.conf ']'
+ for cs_x in '${runlist}'
+ '[' n = y ']'
+ /usr/share/initramfs-tools/hooks/keymap
+ '[' -e /conf/param.conf ']'
+ for cs_x in '${runlist}'
+ '[' n = y ']'
+ /usr/share/initramfs-tools/hooks/legacylvm
+ '[' -e /conf/param.conf ']'
+ for cs_x in '${runlist}'
+ '[' n = y ']'
+ /usr/share/initramfs-tools/hooks/lvm2
+ '[' -e /conf/param.conf ']'
+ for cs_x in '${runlist}'
+ '[' n = y ']'
+ /usr/share/initramfs-tools/hooks/thermal
+ '[' -e /conf/param.conf ']'
+ for cs_x in '${runlist}'
+ '[' n = y ']'
+ /usr/share/initramfs-tools/hooks/udev
+ '[' -e /conf/param.conf ']'
+ for cs_x in '${runlist}'
+ '[' n = y ']'
+ /usr/share/initramfs-tools/hooks/udevhelper
+ '[' -e /conf/param.conf ']'
+ for cs_x in '${runlist}'
+ '[' n = y ']'
+ /usr/share/initramfs-tools/hooks/usplash
+ '[' -e /conf/param.conf ']'
+ for cs_x in '${runlist}'
+ '[' n = y ']'
+ /usr/share/initramfs-tools/hooks/uswsusp
+ '[' -e /conf/param.conf ']'
+ for cs_x in '${runlist}'
+ '[' n = y ']'
+ /usr/share/initramfs-tools/hooks/cryptopenct
+ '[' -e /conf/param.conf ']'
+ for cs_x in '${runlist}'
+ '[' n = y ']'
+ /usr/share/initramfs-tools/hooks/cryptopensc
+ '[' -e /conf/param.conf ']'
+ for cs_x in '${runlist}'
+ '[' n = y ']'
+ /usr/share/initramfs-tools/hooks/cryptpassdev
+ '[' -e /conf/param.conf ']'
+ run_scripts ../../../../tmp/initramfs-tools//hooks
+ initdir=../../../../tmp/initramfs-tools//hooks
+ '[' '!' -d ../../../../tmp/initramfs-tools//hooks ']'
+ get_prereqs
+ set_initlist
+ unset initlist
+ for si_x in '${initdir}/*'
+ '[' '../../../../tmp/initramfs-tools//hooks/*' = '../../../../tmp/initramfs-tools//hooks/*' ']'
+ return
+ reduce_prereqs
+ unset runlist
+ set --
+ i=0
+ '[' 0 -ne 0 ']'
+ call_scripts
+ '[' -e ../../../../tmp/initramfs-tools//DSDT.aml ']'
+ '[' n = y ']'
+ cd /tmp/mkinitramfs_lBresY
+ find .
+ cpio --quiet --dereference -o -H newc
+ gzip
+ '[' -s /tmp/mkinitramfs-OL_ytgKYS ']'
+ '[' n = y ']'
+ rm -rf /tmp/mkinitramfs_lBresY
+ rm -rf /tmp/mkinitramfs-OL_ytgKYS
+ exit 0
